{
  "name": "appium-lg-webos-driver",
  "version": "0.0.1",
  "description": "LG WebOS support for Appium",
  "keywords": [
    "appium",
    "LG",
    "WebOS",
    "testing",
    "automation"
  ],
  "homepage": "https://github.com/headspinio/appium-lg-webos-driver#readme",
  "bugs": {
    "url": "https://github.com/headspinio/appium-lg-webos-driver/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/headspinio/appium-lg-webos-driver.git"
  },
  "license": "Apache-2.0",
  "author": "Jonathan Lipps <jlipps@headspin.io>",
  "types": "./build/lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "lib",
    "build"
  ],
  "scripts": {
    "build": "run-p \"build:*\"",
    "build:distfiles": "babel lib --out-dir=build/lib --copy-files",
    "build:types": "tsc -b",
    "clean": "run-p \"clean:*\"",
    "clean:distfiles": "rimraf \"./build/**/*.js\"",
    "clean:types": "tsc -b --clean",
    "dev": "run-p dev:distfiles dev:types",
    "dev:distfiles": "npm run build:distfiles -- --watch",
    "dev:types": "npm run build:types -- --watch",
    "fix": "npm run lint -- --fix",
    "install-driver": "appium driver install --source=local .",
    "lint": "eslint .",
    "reinstall": "npm run clean && npm ci",
    "prepare": "husky install && npm run build:distfiles && npm run build:types || true",
    "test": "npm run test:unit",
    "test:e2e": "mocha --timeout 40s --slow 10s \"./test/e2e/**/*.spec.js\"",
    "test:unit": "mocha \"./test/unit/**/*.spec.js\""
  },
  "lint-staged": {
    "*.{md,yml,ts}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true
  },
  "dependencies": {
    "@humanwhocodes/env": "^2.2.0",
    "@types/ws": "^8.5.3",
    "appium-chromedriver": "^5.1.1",
    "bluebird": "^3.7.2",
    "get-port": "^5.1.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21",
    "teen_process": "^1.9.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@appium/eslint-config-appium": "^6.0.2",
    "@appium/types": "0.5.0",
    "@babel/cli": "^7.18.2",
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.18.2",
    "@types/babel__register": "^7.17.0",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/sinon-chai": "^3.2.8",
    "@types/teen_process": "^1.16.1",
    "appium": "^2.0.0-beta.46",
    "axios": "^0.27.2",
    "babel-plugin-source-map-support": "^2.2.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "rewiremock": "3.14.3",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.7.4",
    "webdriverio": "7.25.1"
  },
  "peerDependencies": {
    "appium": "^2.0.0-beta.40"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "appium": {
    "driverName": "webos",
    "automationName": "webOS",
    "platformNames": [
      "LGTV"
    ],
    "mainClass": "WebOSDriver"
  }
}
